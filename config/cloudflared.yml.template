# Cloudflared configuration generated by airpod.
# This file is used as a template; runtime config is written to cloudflared.yml.
tunnel: {TUNNEL_ID}
credentials-file: /etc/cloudflared/{TUNNEL_ID}.json
metrics: 0.0.0.0:2000
protocol: auto

originRequest:
  connectTimeout: 30s
  noTLSVerify: true

ingress:
{INGRESS_RULES}  - service: http_status:404
